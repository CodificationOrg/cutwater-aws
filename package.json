{
  "name": "cutwater-aws",
  "version": "0.0.0",
  "description": "A library providing general functionality for TypeScript based AWS projects.",
  "homepage": "https://github.com/CodificationOrg/cutwater-aws",
  "bugs": "https://github.com/CodificationOrg/cutwater-aws/issues",
  "author": {
    "name": "William Weiss",
    "url": "https://github.com/wweiss"
  },
  "license": "Apache-2.0",
  "private": false,
  "main": "_bundles/cutwater-aws.min.js",
  "module": "lib-esm/index.js",
  "types": "lib-esm/index.d.ts",
  "typings": "lib-esm/index.d.ts",
  "scripts": {
    "clean": "gulp clean",
    "validate": "yarn lint && yarn test-xunit && yarn format",
    "test": "gulp prepareTest && ts-node node_modules/tape/bin/tape \"src/**/*.spec.ts\"",
    "test-xunit": "gulp prepareTest && ts-node node_modules/tape/bin/tape \"src/**/*.spec.ts\" | tap-xunit > ./test-results/tap/results.xml",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "docs": "typedoc --out ./lib/docs --excludePrivate --externalPattern **/lib/** --mode file --gitRevision master --name \"CutWater:Core API Documentation\"",
    "publish:docs": "yarn clean && yarn docs && gulp publishDocs",
    "build": "gulp clean && npm-run-all --parallel build:esm build:cjs build:umd",
    "build:esm": "tsc -m es6 --outDir lib-esm",
    "build:cjs": "tsc",
    "build:umd": "webpack --mode=production",
    "release:before": "yarn validate && git add -A .",
    "release:after": "git push origin --tags",
    "release:dev": "yarn release:before && standard-version -a --prerelease beta && yarn release:after",
    "release:prod": "yarn release:before && standard-version -a && yarn release:after && yarn publish:docs",
    "release:dry": "yarn validate && standard-version -a --dry-run --prerelease beta"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.1",
    "@types/node": "^10.11.0",
    "@types/tape": "^4.2.32",
    "awesome-typescript-loader": "^5.2.1",
    "del": "^3.0.0",
    "dotenv": "^6.0.0",
    "gulp": "^4.0.0",
    "gulp-gh-pages": "^0.5.4",
    "gulp-markdown": "^3.0.0",
    "merge2": "^1.2.3",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.0",
    "standard-version": "^4.4.0",
    "tap-xunit": "^2.3.0",
    "tape": "^4.9.1",
    "ts-loader": "^5.2.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.7.0",
    "tslint-config-prettier": "^1.14.0",
    "typedoc": "^0.13.0",
    "typescript": "^3.0.3",
    "terser-webpack-plugin": "^1.2.2",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2"
  },
  "optionalDependencies": {
    "aws-lambda": "^0.1.2",
    "middy": "^0.18.1",
    "gm": "^1.23.1"
  },
  "dependencies": {
    "aws-sdk": "^2.321.0",
    "cutwater-core": "^0.4.0"
  }
}